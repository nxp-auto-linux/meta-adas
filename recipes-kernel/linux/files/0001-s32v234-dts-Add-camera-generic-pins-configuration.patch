From f706306002c7c56e8471ecf189a03c967d59a953 Mon Sep 17 00:00:00 2001
From: Khang Tran <khang.ba.tran@nxp.com>
Date: Wed, 22 Apr 2020 21:31:18 +0700
Subject: [PATCH] s32v234/dts: Add camera generic pins configuration

Signed-off-by: Khang Tran <khang.ba.tran@nxp.com>
Signed-off-by: Dat Vu Van <dat.vu.van@nxp.com>

---
 arch/arm64/boot/dts/freescale/fsl-s32v234-bbmini.dts    | 7 +++++++
 arch/arm64/boot/dts/freescale/fsl-s32v234-evb28899.dtsi | 9 ++++++++-
 arch/arm64/boot/dts/freescale/fsl-s32v234-evb29288.dts  | 7 +++++++
 arch/arm64/boot/dts/freescale/fsl-s32v234-pcie.dtsi     | 9 ++++++++-
 arch/arm64/boot/dts/freescale/fsl-s32v234sbc.dtsi       | 9 ++++++++-
 5 files changed, 38 insertions(+), 3 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-s32v234-bbmini.dts b/arch/arm64/boot/dts/freescale/fsl-s32v234-bbmini.dts
index b1351e0..54fc081
--- a/arch/arm64/boot/dts/freescale/fsl-s32v234-bbmini.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-s32v234-bbmini.dts
@@ -13,6 +13,13 @@

 / {
 	compatible = "fsl,s32v234-bbmini", "fsl,s32v234-evb2016q4", "fsl,s32v234";
+
+	cam_generic{
+        compatible = "fsl,s32v234-cam-generic";
+        power-pins = <0x26 0x1F 0x26 0x1F>;
+        reset-pins = <0x25 0x1E 0x25 0x1E>;
+        status = "okay";
+    };
 };

 &fec {
diff --git a/arch/arm64/boot/dts/freescale/fsl-s32v234-evb28899.dtsi b/arch/arm64/boot/dts/freescale/fsl-s32v234-evb28899.dtsi
index 7044608..d7926f1 100644
--- a/arch/arm64/boot/dts/freescale/fsl-s32v234-evb28899.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-s32v234-evb28899.dtsi
@@ -1,6 +1,6 @@
 /*
  * Copyright 2015-2016 Freescale Semiconductor, Inc.
- * Copyright 2017-2019 NXP
+ * Copyright 2017-2020 NXP
  *
  * This program is free software; you can redistribute it and/or modify
  * it under the terms of the GNU General Public License as published by
@@ -17,6 +17,13 @@
 		stdout-path = "serial0:115200n8";
 	};

+	cam_generic{
+		compatible = "fsl,s32v234-cam-generic";
+		power-pins = <0x25 0x1E 0x25 0x1E>;
+		reset-pins = <0x26 0x1F 0x26 0x1F>;
+		status = "okay";
+	};
+
 	memory_DDR0@80000000 {
 		device_type = "memory";
 		reg = <0 0x80000000 0 0x10000000>;
diff --git a/arch/arm64/boot/dts/freescale/fsl-s32v234-evb29288.dts b/arch/arm64/boot/dts/freescale/fsl-s32v234-evb29288.dts
index 4b839cd..5297a7f
--- a/arch/arm64/boot/dts/freescale/fsl-s32v234-evb29288.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-s32v234-evb29288.dts
@@ -13,6 +13,13 @@

 / {
 	compatible = "fsl,s32v234-evb", "fsl,s32v234-evb2016q4", "fsl,s32v234";
+
+	cam_generic{
+		compatible = "fsl,s32v234-cam-generic";
+		power-pins = <0x25 0x1E 0x25 0x1E>;
+		reset-pins = <0x26 0x1F 0x26 0x1F>;
+		status = "okay";
+	};
 };

 &fec {
diff --git a/arch/arm64/boot/dts/freescale/fsl-s32v234-pcie.dtsi b/arch/arm64/boot/dts/freescale/fsl-s32v234-pcie.dtsi
index 39984e6..03b37eb 100644
--- a/arch/arm64/boot/dts/freescale/fsl-s32v234-pcie.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-s32v234-pcie.dtsi
@@ -1,6 +1,6 @@
 /*
  * Copyright 2015-2016 Freescale Semiconductor, Inc.
- * Copyright 2017-2019 NXP
+ * Copyright 2017-2020 NXP
  *
  * This program is free software; you can redistribute it and/or modify
  * it under the terms of the GNU General Public License as published by
@@ -17,6 +17,13 @@
 		stdout-path = "serial1:115200n8";
 	};

+	cam_generic{
+		compatible = "fsl,s32v234-cam-generic";
+		power-pins = <0x25 0x1E 0x25 0x1E>;
+		reset-pins = <0x26 0x1F 0x26 0x1F>;
+		status = "okay";
+	};
+
 	memory_DDR0@80000000 {
 		device_type = "memory";
 		reg = <0 0x80000000 0 0x10000000>;
diff --git a/arch/arm64/boot/dts/freescale/fsl-s32v234sbc.dtsi b/arch/arm64/boot/dts/freescale/fsl-s32v234sbc.dtsi
index 68ddd69..5eda50a 100644
--- a/arch/arm64/boot/dts/freescale/fsl-s32v234sbc.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-s32v234sbc.dtsi
@@ -1,7 +1,7 @@
 /*
  * Copyright 2015-2016 Freescale Semiconductor, Inc.
  * Copyright 2017 MicroSys Electronics GmbH
- * Copyright 2018-2019 NXP
+ * Copyright 2018-2020 NXP
  *
  * This program is free software; you can redistribute it and/or modify
  * it under the terms of the GNU General Public License as published by
@@ -18,6 +18,13 @@
 		stdout-path = "serial0:115200n8";
 	};

+	cam_generic{
+		compatible = "fsl,s32v234-cam-generic";
+		power-pins = <(-1) (-1) (-1) (-1)>;
+		reset-pins = <0x26 0x1F (-1) (-1)>;
+		status = "okay";
+	};
+
 	memory_DDR0@80000000 {
 		device_type = "memory";
 		reg = <0 0x80000000 0 0x40000000>;
--
2.7.4

